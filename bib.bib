	date-modified = {2024-06-19 03:42:57 +0300},
	doi = {10.1038/s41586-023-06927-3},
	id = {Bluvstein2024},
	isbn = {1476-4687},
	journal = {Nature},
	number = {7997},
	pages = {58--65},
	title = {Logical quantum processor based on reconfigurable atom arrays},
	url = {https://doi.org/10.1038/s41586-023-06927-3},
	volume = {626},
	year = {2024},
	bdsk-url-1 = {https://doi.org/10.1038/s41586-023-06927-3}}

